<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  
  
  <!--Set button inner-shadow-->
  
  <!--Layer 0 (darkest color)-->
  <item>
    <shape>
      <solid android:color="#bcbcbc"/>
      <padding android:top="1dp" 
               android:right="0dp" 
               android:bottom="1dp" 
               android:left="0dp" />
    </shape>
  </item>

